<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.swago.seenthemlive">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ui.setlist.SetlistDetailActivity"
            android:label="@string/title_activity_setlist_detail"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name=".ui.search.SearchResultActivity" />
        <activity android:name=".ui.search.SearchActivity"
            android:label="Search"/>
        <activity
            android:name=".ViewConcertsActivity"
            android:label="@string/title_activity_view_concerts"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ui.common.BaseActivity" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>